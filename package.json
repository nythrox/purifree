{
  "name": "purifree-ts",
  "version": "0.16.0-beta.10",
  "description": "Point-free Functional programming library for TypeScript",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "https://github.com/nythrox/purifree.git",
  "author": "nythrox <jasonsantiagobutler@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && tsc -p tsconfig.es.json",
    "docs": "typedoc --out documentation/ src/ --theme minimal",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "src/.+\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "jest": "27.5.1",
    "prettier": "2.6.0",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "purify-ts": "^1.2.0"
  }
}
